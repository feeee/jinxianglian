/* pages/renmai/index.wxss */
.nav{height: 260rpx; border-top:solid #dfdfdf 2rpx;border-bottom:solid #dfdfdf 2rpx;display: flex;align-items: center;flex-wrap: wrap;}
.nav_list{display: flex;flex-direction: column;align-items: center;align-self: center;font-size:28rpx;width: 25%;}
.nav_list image{width: 48rpx;height: 48rpx;}
.nav_list text{margin-top: 10rpx;}
.contenter{border-top: 2rpx solid #dddddd;border-bottom: 2rpx solid #dddddd;display: flex;flex-direction: column;justify-content: space-between;}
.experience_box{display: flex; justify-content: space-around}
.experience_box>view{display: flex;width: 704rpx;padding: 35rpx 0;border-bottom:2rpx solid #e9e9e9;}

.experience_box image{width: 100rpx;height: 100rpx;}
.experience{display: flex;flex-direction: column;margin-left: 25rpx; }
.experience{font-size: 30rpx;}
.contenter .experience_box:last-child>view{border:none; }
button{position: fixed;bottom: 0;width: 100%;height: 100rpx;display: flex;justify-content: center;align-items: center;}
page{padding-bottom: 110rpx;}
