.dyh{width: 12rpx;height: 21rpx;}
.content .list:last-child{border:none;}
.star{height: 65rpx;display: flex;align-items: center;font-size: 30rpx;color: #2387e2;background: #f8f8f8;justify-content: center;}
.star text{display: inline-flex;width: 704rpx;line-height: 65rpx;}
.star_box{font-size: 28rpx;display: flex;flex-wrap:wrap;padding-bottom:30rpx;flex: 2; }
.star_list{margin-top: 30rpx;margin-left:30rpx; padding: 16rpx 36rpx;background: #f5f5f5;border-radius: 15rpx;color: #7e7e7e;}
















