
.content{display: flex;justify-content: center;}
.content_box{width: 704rpx;padding: 30rpx 0rpx;display: flex;font-size: 25rpx;line-height: 50rpx;color: #b9b9b9;}
.content_box image{width: 129rpx;height: 129rpx;}
.info{margin-left: 24rpx;width: 392rpx;}
.salary{display: flex;flex-direction: column;}
.salary text{justify-content: flex-end;display: flex;}

.senior{flex-direction: column;justify-content: center;align-items: center;font-size: 30rpx;}
.senior_text{width: 704rpx;height: 35rpx;line-height: 35rpx;}

.senior_list_box{width: 704rpx;display: flex;justify-content: flex-start;flex-wrap: wrap;margin-bottom: 30rpx;align-items: center;}
.senior_list{height: 57rpx;width: 207rpx;border: 2rpx solid #e8e8e8;display: flex;margin-right: 22rpx; justify-content: center;align-items: center;margin-top: 20rpx;border-radius: 10rpx;}
 /* radio{display: none;}  */
.but{width: 704rpx;display: flex;justify-content: space-around;}
.but button{width: 330rpx;height: 80rpx;display: flex;justify-content: center;align-items: center;color: #fff;background: #2f91e8;}
.photo{width: 125rpx;height: 125rpx;border-radius: 50%;}
.group{width: 100%;justify-content: center;border-top: 2rpx solid #eee;padding: 20rpx 0rpx;}
.group_list{display: flex;width: 704rpx;justify-content: space-between;}
.list_left{display: flex;font-size: 28rpx;line-height: 46rpx;}

.xiao_bg{width: 85rpx;display: flex;justify-content: center;align-items: center;background: #86abdc;}
.xiao{width: 26rpx;height: 26rpx;}
.list_right{font-size: 26rpx;color: #949494;}