/* pages/star/shenqing/index.wxss */
.page{display: flex;flex-direction: column;justify-content: center;align-items: center;font-size: 28rpx;}
.info_box{width: 704rpx;margin-top: 20rpx;display: flex;flex-direction: column;line-height: 40rpx;}
.inf_header{font-size: 32rpx;color: #4697e0;line-height: 75rpx;}
.box{margin-top: 40rpx;}
.city{border: 2rpx solid #e0e0e0; width:664rpx;padding: 0rpx 20rpx; height: 70rpx;display: flex;align-items: center;justify-content: space-between;border-radius: 8rpx;margin-bottom: 20rpx;}
.san{width: 17rpx;height: 11rpx;}
.mode{position: fixed;height: 100%;width: 100%;background: rgba(0, 0, 0, .3);top: 0rpx;}
.shijian{z-index: 2;position: fixed;bottom: 0;width: 100%;}
.que{background: white;width: 100%;height: 50rpx;font-size: 32rpx;color:#4697e0;display: flex;justify-content: space-between; }
.que text{margin: 0rpx 20rpx;}
/*lingyuxuanze  */
.field{position: fixed;bottom: 0;z-index: 2;height: 300rpx;width: 100%;}
.jifen{height: 70rpx;display: flex;justify-content: space-between;width: 704rpx;align-items: center;font-size: 30rpx;color: #4f4f4f;}
.jifen text{background: #eee;width: 47%;display: flex;justify-content: center;align-items: center;height: 70rpx;border-radius: 8rpx;border: 2rpx solid #e1e1e1;overflow: hidden;white-space: nowrap;text-overflow:ellipsis;-webkit-line-clamp: 1;}
.but{width: 704rpx;height: 70rpx;background: #2e94e9;display: flex;justify-content: center;align-items: center;color: white;margin-top: 60rpx;border-radius: 10rpx;font-size: 32rpx;}